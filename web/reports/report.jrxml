	<queryString>
		<![CDATA[SELECT Name as ProblemType, COALESCE(m, 0), COALESCE(f, 0) , (COALESCE(m, 0) + COALESCE(f, 0) )
FROM (
SELECT Name
FROM tbl_dropdown
WHERE dropdownType =  'Problem_type'
) AS t1
LEFT JOIN (
SELECT Chief_problem, SUM( IF( Gender =  'Male', 1, 0 ) ) AS m, SUM( IF( Gender =  'Female', 1, 0 ) )  AS f
FROM tbl_problem
LEFT JOIN tbl_worker ON Worker_FIN_number = FIN_number
WHERE Gender =  'Male'
GROUP BY Chief_problem
) AS t2 ON ( t1.Name = t2.Chief_problem )]]>
	</queryString>
	<field name="ProblemType" class="java.lang.String"/>
	<field name="COALESCE(m, 0)" class="java.math.BigDecimal"/>
	<field name="COALESCE(f, 0)" class="java.math.BigDecimal"/>
	<field name="(COALESCE(m, 0) + COALESCE(f, 0) )" class="java.math.BigDecimal"/>
